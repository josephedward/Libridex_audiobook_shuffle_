{
  "name": "libridex_audiobook_shuffle",
  "version": "1.0.0",
  "description": "Audiobook Shuffling App",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "./a.sh && concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Joseph Edward",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint-plugin-react-hooks": "^2.0.1",
    "nodemon": "^1.18.7",
    "redux-devtools-extension": "^2.13.8"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.6.10",
    "morgan": "^1.9.1",
    "node": "^10.16.1",
    "node-linux-x64": "^12.9.1",
    "npm": "^6.11.2",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "qs": "^6.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-sticky-footer": "0.1.0-rc3",
    "react-textarea-autosize": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.87.3",
    "typescript": "^3.5.3",
    "validator": "^11.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}
